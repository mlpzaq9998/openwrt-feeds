	option enable '0'
	option pdnsd_enable '1'
	option tunnel_forward '8.8.4.4:53'
	option run_mode 'gfw'
	option enable_switch '0'
	option enable_ssr_local '0'
	option subscribe_enable '0'
	option subscribe_update_time '2'
